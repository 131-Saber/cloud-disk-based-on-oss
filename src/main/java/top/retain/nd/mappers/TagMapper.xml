<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.retain.nd.mapper.ITagMapper">

    <select id="getTagById" resultType="top.retain.nd.entity.Tag">
        select * from tag where user_id = #{userId} and id =  #{id}
    </select>


</mapper>
